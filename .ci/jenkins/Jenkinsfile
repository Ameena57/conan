                            echo "Run unittests"
                        }
                    }
                    stage ('py38') {
                        agent any
                        script {
                            echo "Run unittests"
                        }
                    }
                }
            }
        }
        stage ('Functional') {
            script {
                echo "Run functional tests"
            }
        }
    }
}
